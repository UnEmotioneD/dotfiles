# --- Python venv ---
alias von=". ~/venv/robot_venv/bin/activate"
alias voff="deactivate"

# --- ROS2 development environment ---

# "https://docs.ros.org/en/jazzy/Installation/Ubuntu-Install-Debs.html"

# separate domain from unwanted connection
export ROS_DOMAIN_ID=24

# set up your environment
source /opt/ros/jazzy/setup.zsh
#alias jazzy="source /opt/ros/jazzy/setup.zsh"

# --- Pinky ---
alias sp="ssh pinky@192.168.4.1"
alias riv="ros2 launch pinky_navigation map_view.launch.xml"
alias teleop="ros2 run teleop_twist_keyboard teleop_twist_keyboard"
alias loadmap="ros2 launch pinky_navigation nav2_view.launch.xml"

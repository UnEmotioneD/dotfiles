{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/master/doc/json_schema.json",
  "logo": {
    "type": "small",
  },
  "modules": [
    "title",
    "separator",
    "os",
    "host",
    "kernel",
    "packages",
    "shell",
    "uptime",
    {
      "type": "memory",
      "format": "{used} / {total} ",
    },
    "break",
    {
      "type": "colors",
      "block": {
        "range": [0, 7],
      },
    },
  ],
}
